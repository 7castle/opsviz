#!/bin/bash
while [ `ls -t /opt/aws-billing//processing/*.csv | wc -l` -ge 1 ]; do
    NewFile=`ls -t /opt/aws-billing//processing/*.csv | head -1`
    if [ -f "${NewFile}.processed" ]
    then
    	mv ${NewFile} ${NewFile}.processed
    else
    	#/opt/aws-billing/aws-billing -accounts-file="/opt/aws-billing/aws-accounts-file.csv" -concurrency=4 -file="${NewFile}" -logstash-address="logstash1:5140"
    	mv ${NewFile} ${NewFile}.processed
    fi
done